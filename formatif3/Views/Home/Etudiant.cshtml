
@{
    ViewBag.Title = "Etudiant";
    Layout = "~/Views/Shared/_Layout3.cshtml";
}

<h1 style="margin-top:30px;">Etudiant</h1>
<p>Veuillez vous inscrire</p>

@{

    Html.BeginForm("etudiantNew", "Home", FormMethod.Post);

}

<p>Votre nom : @Html.TextBox("nom")</p>

<p>Programme : @Html.TextBox("prog")</p>

<p>Année d'étude : @Html.TextBox("etude")</p>

<p>Année de naissance : @Html.TextBox("nais")</p>


<input type="submit" value="M'inscrire" />

@{

    Html.EndForm();

}